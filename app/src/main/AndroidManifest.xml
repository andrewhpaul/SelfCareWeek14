<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.KA2001.selfcare">

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.KA2001.selfcare.splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.KA2001.selfcare.view.ProductsGroupActivity"></activity>
        <activity android:name="com.KA2001.selfcare.view.CreateGroupActivity" />
        <activity android:name="com.KA2001.selfcare.view.GroupsActivity" />
        <activity android:name="com.KA2001.selfcare.view.ProductDetailsActivity" />
        <activity android:name="com.KA2001.selfcare.view.AddExpiryNotificationActivity" />
        <activity android:name="com.KA2001.selfcare.view.SelecteNotificationProduct" />
        <activity android:name="com.KA2001.selfcare.view.SelectGroupProductActivity" />
        <activity android:name="com.KA2001.selfcare.view.NotificationActivity" />
        <activity android:name="com.KA2001.selfcare.view.AddProductActivity" />
        <activity android:name="com.KA2001.selfcare.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name="com.KA2001.selfcare.Controller.NotificationReciver" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>